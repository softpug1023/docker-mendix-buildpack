{
  "name": "@wecom/jssdk",
  "version": "2.3.1",
  "homepage": "https://github.com/TencentWecom/wecom-jssdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TencentWecom/wecom-jssdk.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": {
        "development": "./dist/wecom.js",
        "default": "./dist/wecom.prod.js"
      },
      "require": {
        "development": "./dist/wecom.cjs",
        "default": "./dist/wecom.prod.cjs"
      }
    },
    "./types/*": "./types/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "main": "dist/wecom.prod.cjs",
  "jsdelivr": "dist/wecom.global.prod.js",
  "unpkg": "dist/wecom.global.prod.js",
  "module": "dist/wecom.prod.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:rollup": "rollup -c rollup.config.js",
    "build:types": "dts-bundle-generator --config dts.config.cjs",
    "clear": "del-cli dist types",
    "dev": "npm run clear && concurrently npm:dev:*",
    "dev:rollup": "rollup -c rollup.config.js -w",
    "dev:types": "npm-watch build:types",
    "lint": "concurrently npm:lint:*",
    "lint:script": "eslint .",
    "lint:types": "tsc --noEmit",
    "pre": "pnpm changeset pre",
    "test": "vitest run --coverage",
    "test:dev": "vitest --coverage",
    "test:ui": "vitest --coverage --ui"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.0",
    "@changesets/cli": "^2.27.7",
    "@eslint/js": "^9.6.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-swc": "^0.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@swc/core": "^1.6.6",
    "@swc/helpers": "^0.5.11",
    "@types/node": "^20.14.9",
    "@vitest/coverage-istanbul": "^2.1.1",
    "@vitest/ui": "^2.1.2",
    "concurrently": "^8.2.2",
    "deep-patch-data": "^1.0.0",
    "del-cli": "^6.0.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.7.0",
    "jsdom": "^25.0.1",
    "npm-watch": "^0.13.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0",
    "vitest": "^2.1.1"
  },
  "watch": {
    "build:types": {
      "patterns": [
        "src/**/*.ts"
      ],
      "extensions": "ts"
    }
  }
}
